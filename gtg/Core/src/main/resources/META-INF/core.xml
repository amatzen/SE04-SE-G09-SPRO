<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.osgi.org/xmlns/scr/v1.1.0" name="dk.sdu.mmmi.swe.gtg.core.internal.main.Game">
    <implementation class="dk.sdu.mmmi.swe.gtg.core.internal.main.Game"/>

    <reference bind="setEngine" cardinality="1..1"
               interface="dk.sdu.mmmi.swe.gtg.common.services.managers.IEngine"
               name="IEngine" policy="dynamic" unbind="removeEngine"/>

    <reference bind="addGamePluginService" cardinality="0..n"
               interface="dk.sdu.mmmi.swe.gtg.common.services.plugin.IGamePluginService"
               name="IGamePluginService" policy="dynamic" unbind="removeGamePluginService"/>

    <reference bind="setWorldManager" cardinality="1..1"
               interface="dk.sdu.mmmi.swe.gtg.worldmanager.services.IWorldManager"
               name="IWorldManager" policy="dynamic" unbind="removeWorldManager"/>
</component>